<reactiveUi:ReactiveWindow
    x:Class="Interface.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mainScreen="clr-namespace:Interface"
    xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:reactiveUi="http://reactiveui.net"
    xmlns:userControls="clr-namespace:Interface.UserControls"
    Title="MainWindow"
    Width="300"
    Height="100"
    x:TypeArguments="mainScreen:MainViewModel"
    Background="{StaticResource BackgroundColor1}"
    ResizeMode="NoResize"
    Topmost="True"
    WindowStyle="None"
    mc:Ignorable="d">
    <Grid>
        <userControls:LibraryView x:Name="LibraryView" />
    </Grid>
</reactiveUi:ReactiveWindow>
